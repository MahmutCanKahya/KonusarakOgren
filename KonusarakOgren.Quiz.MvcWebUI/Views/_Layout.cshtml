<!DOCTYPE html>

<html>
<head>
    <title>Little Closet</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Little Closet template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href=@(Url.Content("~/styles/bootstrap-4.1.2/bootstrap.min.css"))>
    <link href=@(Url.Content("~/plugins/font-awesome-4.7.0/css/font-awesome.min.css")) rel="stylesheet" type="text/css">
    <link href="/node_modules/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <script src="/node_modules/jquery/dist/jquery.min.js"></script>
    <script src="/node_modules/bootstrap/dist/js/bootstrap.js"></script>
    <script src="/node_modules/jquery-validation/dist/jquery.validate.js"></script>
    <script src="/node_modules/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
</head>
<body>
    <div class="container">
        @RenderBody()
        @if (TempData["message"] != null)
        {
            <div class="alert alert-info" role="alert">
                @TempData["message"]
            </div>
        }
    </div>
    
    <script src=@(Url.Content("~/js/jquery-3.2.1.min.js"))></script>
    <script src=@(Url.Content("~/styles/bootstrap-4.1.2/popper.js"))></script>
    <script src=@(Url.Content("~/styles/bootstrap-4.1.2/bootstrap.min.js"))></script>
</body>
</html>
